package practice;

public class Chapter0701 {
	public static void main(String[] args) {
		String light = red;
		switch(light) {
		case red:
			System.out.println("赤信号です");
			break;
		case yellow:
			System.out.println("黄信号です");
			break;
		case blue:
			System.out.println("青信号");
			break;
		default:
			System.out.println("信号の色ではありません");
		}
	}

}
